<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>SideLine</display-name>
  <filter>
    <filter-name>SideLineFilter</filter-name>
    <filter-class>com.sideline.filter.SideLineFilter</filter-class>
    <init-param>
      <param-name>noPath</param-name>
      <param-value>index.jsp;resister.jsp;appeal.jsp;HomePage.css;LoginServlet;ResisterServlet;ImageServlet;staticFile</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>SideLineFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <listener>
    <listener-class>com.sideline.listener.MySqlListener</listener-class>
  </listener>
</web-app>